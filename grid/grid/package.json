    "build:webpack:prod": "npm run remove-built-files && webpack --env=prod --progress --profile --colors -p",
    "build:webpack:prod:server": "npm run remove-built-files && webpack --env=prod --progress --profile --colors -p  && webpack-dev-server --env=prod",
